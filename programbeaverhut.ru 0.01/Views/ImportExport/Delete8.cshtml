@model programbeaverhut.ru.Models.Contract
@{
    ViewBag.Title = "Удаление договора";
}

<div class="card border-primary" style="margin: 0px 0px 15px 0px;">

    <div class="card-header">
        <form class="form-row align-items-center ">
            <h4>Удаление категории "@Html.DisplayFor(Contract => Contract.TitleName)"</h4>
        </form>
    </div>
    <div class="card-body">

        <h6>Данные нельзя будет востоновить! Проверьте данные перед удалением.</h6>
        <div style="margin: 0px 0px 10px 0px;">
            <a>The data cannot be restored!</a>
        </div>

        <dl class="dl-horizontal">
            <dt> Загаловок</dt>
            <dd>
                @Html.DisplayFor(Contract => Contract.TitleName)
            </dd>

            <dt>Адрес заключения договора</dt>
            <dd>
                @Html.DisplayFor(Contract => Contract.Address)
            </dd>
        </dl>

        <div>
            <form asp-controller="ImportExport" asp-action="Delete8" method="post">
                <div class="form-group">
                    <input type="submit" class="form-group btn btn-outline-dark" value="Удалить" />
                </div>
            </form>
        </div>
    </div>
</div>
